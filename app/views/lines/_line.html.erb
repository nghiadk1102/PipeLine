<tr>
	<td><%= line.id %></td>
  <td><%= line.name %></td>
  <td><%= line.created_at %></td>
  <td><%= line.description %></td>
  <td>
  	<button class="fa fa-eye button_action show-line" data-id="<%= line.id %>" data-toggle="modal" data-target="#show-map-line"></button>
  	<button class="button_action"><%= link_to "", line_path(line), confirm: "Are you sure?", method: :delete, class: "fa fa-trash" %></button>
  </td>
</tr>